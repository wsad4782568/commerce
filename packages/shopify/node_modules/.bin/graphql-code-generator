#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/edy/Desktop/yxy/个人项目/马蒂尼海外站/commerce/node_modules/.pnpm/@graphql-codegen+cli@2.7.0_@types+node@18.7.18_graphql@16.6.0_typescript@4.8.3/node_modules/@graphql-codegen/cli/node_modules:/Users/edy/Desktop/yxy/个人项目/马蒂尼海外站/commerce/node_modules/.pnpm/@graphql-codegen+cli@2.7.0_@types+node@18.7.18_graphql@16.6.0_typescript@4.8.3/node_modules/@graphql-codegen/node_modules:/Users/edy/Desktop/yxy/个人项目/马蒂尼海外站/commerce/node_modules/.pnpm/@graphql-codegen+cli@2.7.0_@types+node@18.7.18_graphql@16.6.0_typescript@4.8.3/node_modules:/Users/edy/Desktop/yxy/个人项目/马蒂尼海外站/commerce/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/edy/Desktop/yxy/个人项目/马蒂尼海外站/commerce/node_modules/.pnpm/@graphql-codegen+cli@2.7.0_@types+node@18.7.18_graphql@16.6.0_typescript@4.8.3/node_modules/@graphql-codegen/cli/node_modules:/Users/edy/Desktop/yxy/个人项目/马蒂尼海外站/commerce/node_modules/.pnpm/@graphql-codegen+cli@2.7.0_@types+node@18.7.18_graphql@16.6.0_typescript@4.8.3/node_modules/@graphql-codegen/node_modules:/Users/edy/Desktop/yxy/个人项目/马蒂尼海外站/commerce/node_modules/.pnpm/@graphql-codegen+cli@2.7.0_@types+node@18.7.18_graphql@16.6.0_typescript@4.8.3/node_modules:/Users/edy/Desktop/yxy/个人项目/马蒂尼海外站/commerce/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@graphql-codegen/cli/bin.js" "$@"
else
  exec node  "$basedir/../@graphql-codegen/cli/bin.js" "$@"
fi
