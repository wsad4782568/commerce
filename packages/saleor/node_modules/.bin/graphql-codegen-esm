    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/edy/Desktop/yxy/个人项目/马蒂尼海外站/commerce/node_modules/.pnpm/@graphql-codegen+cli@2.12.0_@types+node@17.0.45_graphql@16.6.0_ts-node@10.9.1_typescript@4.8.3/node_modules/@graphql-codegen/cli/esm/node_modules:/Users/edy/Desktop/yxy/个人项目/马蒂尼海外站/commerce/node_modules/.pnpm/@graphql-codegen+cli@2.12.0_@types+node@17.0.45_graphql@16.6.0_ts-node@10.9.1_typescript@4.8.3/node_modules/@graphql-codegen/cli/node_modules:/Users/edy/Desktop/yxy/个人项目/马蒂尼海外站/commerce/node_modules/.pnpm/@graphql-codegen+cli@2.12.0_@types+node@17.0.45_graphql@16.6.0_ts-node@10.9.1_typescript@4.8.3/node_modules/@graphql-codegen/node_modules:/Users/edy/Desktop/yxy/个人项目/马蒂尼海外站/commerce/node_modules/.pnpm/@graphql-codegen+cli@2.12.0_@types+node@17.0.45_graphql@16.6.0_ts-node@10.9.1_typescript@4.8.3/node_modules:/Users/edy/Desktop/yxy/个人项目/马蒂尼海外站/commerce/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/edy/Desktop/yxy/个人项目/马蒂尼海外站/commerce/node_modules/.pnpm/@graphql-codegen+cli@2.12.0_@types+node@17.0.45_graphql@16.6.0_ts-node@10.9.1_typescript@4.8.3/node_modules/@graphql-codegen/cli/esm/node_modules:/Users/edy/Desktop/yxy/个人项目/马蒂尼海外站/commerce/node_modules/.pnpm/@graphql-codegen+cli@2.12.0_@types+node@17.0.45_graphql@16.6.0_ts-node@10.9.1_typescript@4.8.3/node_modules/@graphql-codegen/cli/node_modules:/Users/edy/Desktop/yxy/个人项目/马蒂尼海外站/commerce/node_modules/.pnpm/@graphql-codegen+cli@2.12.0_@types+node@17.0.45_graphql@16.6.0_ts-node@10.9.1_typescript@4.8.3/node_modules/@graphql-codegen/node_modules:/Users/edy/Desktop/yxy/个人项目/马蒂尼海外站/commerce/node_modules/.pnpm/@graphql-codegen+cli@2.12.0_@types+node@17.0.45_graphql@16.6.0_ts-node@10.9.1_typescript@4.8.3/node_modules:/Users/edy/Desktop/yxy/个人项目/马蒂尼海外站/commerce/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@graphql-codegen/cli/esm/bin.js" "$@"
else
  exec node  "$basedir/../@graphql-codegen/cli/esm/bin.js" "$@"
